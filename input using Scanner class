import java.util.Scanner;
public class Main{
     public static void main(String[]args){
          int a,b,sum;
          Scanner sc=new Scanner(System.in);
          System.out.println("enter the value of a");
          a=sc.nextInt();
          System.out.println("enter the value of b");
          b=sc.nextInt();
          sum=a+b;
          System.out.println("sum="+sum);
          
     }
}
